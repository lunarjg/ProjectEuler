import math

result = set()
max = 0
max_value = 0

for p in range(1, 1001) :
  for a in range(1, 501) :
    for b in range(a, 501) :
      h = math.sqrt(a ** 2 + b ** 2)
      if h == math.floor(h) and a + b + h == p :
        result.add((a, b, int(h)))
  if len(result) > max :
    max = len(result)
    max_value = p

print(max_value)
