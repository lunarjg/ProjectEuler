max = 0
nums = [x for x in range(1,10)]

for digit in range(1, 5) :
  for num in range(10 ** (digit - 1), 10 ** digit) :
    mul = 1
    digits = []
    while len(digits) < 9 :
      digits += [int(x) for x in str(num * mul)]
      mul += 1
    if sorted(digits) == nums :
      pandigital_num = int(''.join([str(x) for x in digits]))
      if pandigital_num > max :
        max = pandigital_num

print(max)
