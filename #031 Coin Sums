wallet = [100, 50, 20, 5, 2, 1]
money = 200

def coin_ways_num(money, wallet) :
  ways = 0
  coin = wallet[0]
  if money == 0 :
    return 1
  else :
    if len(wallet) == 1:
      return 1
    else :
      for i in range(0, money // coin + 1) :
        ways += coin_ways_num(money - i * coin, wallet[1:])
    return ways

print(coin_ways_num(money, wallet))
