import math
from fractions import Fraction

a = [2,1]
for k in range(1, 34) :
  a+=[2*k, 1, 1]

def seq(n) :
  if n == 1 :
    return a[0]
  else :
    value = a[n-1]
    for k in range(n - 2, -1 , -1):
      value = Fraction(1, value)
      value += a[k]
    return value

print(sum(int(x) for x in str(seq(100).numerator)))
