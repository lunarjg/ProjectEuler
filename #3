import math

n = 600851475143
p_facs = []
prime = []

for i in range(2, math.floor(n ** 0.5)+1) : 
  if all(i % j != 0 for j in range(2, math.floor(i ** 0.5)+1)) == True :
    prime.append(i)

for i in prime :
  while n % i == 0 :
    n = n / i 
    p_facs.append(i)

p_facs.sort()

print(p_facs[-1])
