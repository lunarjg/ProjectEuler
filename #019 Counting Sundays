calendar = [31, 28, 31 ,30, 31, 30, 31, 31, 30, 31, 30, 31]

start_year = 1901
count = 0
start_point = 6
week = 7

for year in range(start_year, 2001) :
  if year % 4 == 0 and year % 400 != 0 :
    calendar[1] = 29
  else :
    calendar[1] = 28

  for month in calendar :
    if start_point == 1 :
      count += 1
    while start_point <= month :
      start_point += week
    start_point %= month
print(count)
