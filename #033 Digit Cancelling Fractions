numerator = [x for x in range(11, 100) if x % 10 != 0] # 2-digit nums, except for 10 multiples
nums = [x for x in range(1,10)]

fractions = list()

for i in numerator :
  denominator = [int(x + y) for x in list(map(str, nums)) for y in str(i)] + [int(y + x) for x in list(map(str, nums)) for y in str(i)] # nums bigger than numerator

  for j in denominator :
    i_digits = [x for x in str(i)]
    j_digits = [x for x in str(j)]
    intersection = list((set(i_digits) & set(j_digits)))

    for x in intersection :
      if i * int(str(j).replace(x, '')) == j * int(str(i).replace(x, '')) :
        fractions.append((i,j))

print(fractions)
