from numba import jit

@jit(nopython = True)
def tri_num(L) :
  count = 0
  flag = False
  for a in range(1, L-1) : 
    for b in range(1, a) :
      if a ** 2 + b ** 2 ==  (L - (a+b)) ** 2 :
        count += 1
      if count > 1 :
        flag = True
        break
    if flag == True :
      break

  return count

result = 0

for L in range(2, 1500000, 2) :
  print(L)
  if tri_num(L) == 1 :
    result += 1

print(result)
