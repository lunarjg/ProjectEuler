import math

def find_divisors_num(n) :
  nums = [2] +[2*x + 1 for x in range(1, math.floor(n ** 0.5) + 1)]
  divisors = []
  div_count = []
  for k in nums :
    if n % k == 0 :
      divisors.append(k)
      n = int(n / k)
  print(divisors)
  for x in divisors :
    div_count.append(divisors.count(x) + 1)
  return math.prod(div_count)

print(find_divisors_num(28))

