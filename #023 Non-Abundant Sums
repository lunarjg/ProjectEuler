import math

def d(n) :
  divisors = {1}
  for i in range(2, math.isqrt(n) + 1) :
    if n % i == 0 :
      divisors.update({i, n / i})
  return int(sum(divisors))

abundant_nums = [n for n in range(12, 14062) if d(n) > n]
abundant_sums = set()

for i in abundant_nums :
  for j in abundant_nums :
    abundant_sums.add(i+j)

print(sum({x for x in range(1,28122)}.difference(abundant_sums)))
