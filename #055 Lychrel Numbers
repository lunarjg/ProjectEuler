def reverse_num(n) :
  n = str(n)
  digit = [i for i in str(n)]
  return int(''.join(digit[::-1]))

def is_palindrome_num(n) :
  if n == reverse_num(n) :
    return True
  else :
    return False

result = 0

for n in range(1, 10000) :
  for _ in range(50) :
    n += reverse_num(n)
    if is_palindrome_num(n) :
      break
  else :
    result += 1

print(result)
